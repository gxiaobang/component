{
    "presets": [
        "es2015",
        "stage-2",
        "react"
    ],
    "plugins": [
        ["transform-decorators-legacy"],
        [
            "transform-runtime",
            {
                "helpers": false, 
                "polyfill": false, 
                "regenerator": true, 
                "moduleName": "babel-runtime" 
            }
        ],
        [
            "import",
            [
                {
                  "libraryName": "compontents"
                },
                {
                  "libraryName": "base"
                }
            ]
        ],
        [
            "webpack-alias",
            {
              "config": "webpack.config.dev.js"
            }
        ]
    ]
}