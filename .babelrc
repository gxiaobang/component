{
	"presets": [
		"es2015",
		"stage-2",
		"react"
	],
	"env": {
		"development": {
			// "presets": ["react-hmre"],
			"plugins": ["react-hot-loader/babel"]
		}
	},
	"plugins": [
		["transform-decorators-legacy"],
		[
			"transform-runtime",
			{
				"helpers": false, 
				"polyfill": false, 
				"regenerator": true, 
				"moduleName": "babel-runtime" 
			}
		],
		[
			"webpack-alias",
			{
				"config": "webpack.config.dev.js"
			}
		]
	]
}